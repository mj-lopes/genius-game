//  Visit https://aka.ms/tsconfig.json to read more about this file
{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "moduleResolution": "Node",
    "rootDir": "./",
    "outDir": "./script",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
